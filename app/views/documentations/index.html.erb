<h1>Orphan Documentations</h1>

<table id="documentations-table">
  <thead>
    <tr>
      <th>Filename</th>
      <th>Text</th>
    </tr>
  </thead>
  <tbody>
  <% @documentations.each do |documentation| %>
    <tr>
      <td>
        <%= documentation.media.path.split("/").last %>
      </td>
      <td>
        <%= link_to "show", documentation_url(documentation.id) if documentation.paper %>
      </td>
    </tr>
  <% end %>
  </tbody>
</table>